									"function findAddedEntity(model) {",
									"    return model['@odata.type'] === '#Sitecore.Commerce.Core.PersistedEntityModel' && model['Name'] === 'Sitecore.Commerce.Plugin.Inventory.InventorySet';",
									"}",
									"",
									"postman.setGlobalVariable(\"InventorySetId\", jsonData.Models.find(findAddedEntity).EntityId);",
